
let output = if (let fib_number = {
        let n = 14;

        let mut prevprevn = 0;
        let mut prevn = 0;
        let mut curn = 1;

        let mut i = 0;

        while i <= n {

            prevprevn = prevn;

            prevn = curn;

            curn = prevprevn + prevn;

            i = i + 1;

        };

        return curn;
    }) != 987 {
        fib_number
    } else {
        -fib_number
    };

print(output);